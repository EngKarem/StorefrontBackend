{
  "name": "storefrontbackend",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./build --onSuccess \"node ./build/server.js\"",
    "jasmine": "jasmine-ts",
    "build": "npx tsc",
    "start": "npm run build && node build/server",
    "migrationup": "db-migrate up",
    "test": "db-migrate db:create test && cross-env ENV=test db-migrate --env test up && jasmine-ts && db-migrate db:drop test",
    "postinstall": "npx json -I -f ./node_modules/jasmine/package.json -e \"delete this.exports\""
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.0",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.7.3",
    "sequelize": "^6.21.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.6.5",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "bcrypt": "^5.0.1",
    "jasmine": "^4.3.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  }
}
